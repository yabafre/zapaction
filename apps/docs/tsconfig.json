{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true
  },
  "include": [
    "next-env.d.ts",
    "app",
    "components",
    "middleware.ts",
    ".next/types/**/*.ts",
    "theme.config.tsx",
    "mdx-components.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}